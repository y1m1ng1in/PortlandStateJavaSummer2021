<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Appointment Book REST Service</display-name>
  <servlet>
    <servlet-name>AppointmentBookServlet</servlet-name>
    <servlet-class>edu.pdx.cs410J.yl6.AppointmentBookServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AppointmentBookServlet</servlet-name>
    <url-pattern>/appointments</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserRegistrationServlet</servlet-name>
    <servlet-class>edu.pdx.cs410J.yl6.UserRegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserRegistrationServlet</servlet-name>
    <url-pattern>/registration</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserLoginServlet</servlet-name>
    <servlet-class>edu.pdx.cs410J.yl6.UserLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserLoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BookAppointmentServlet</servlet-name>
    <servlet-class>edu.pdx.cs410J.yl6.BookAppointmentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BookAppointmentServlet</servlet-name>
    <url-pattern>/book</url-pattern>
  </servlet-mapping>
</web-app>
